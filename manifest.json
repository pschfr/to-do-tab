{
	"manifest_version": 2,
	"name": "To Do New Tab Page",
	"description": "A new tab replacement to keep on top of your to do list.",
	"version": "0.0.1",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"permissions": [
		"storage"
	],
	"content_security_policy": "script-src 'self' https://unpkg.com; object-src 'self'"
}
